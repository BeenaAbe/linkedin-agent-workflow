# Streamlit Secrets Configuration Example
# Copy this file to .streamlit/secrets.toml and fill in your actual API keys
# NEVER commit the actual secrets.toml file!

[secrets]
NOTION_TOKEN = "secret_YOUR_NOTION_INTEGRATION_TOKEN"
NOTION_DATABASE_ID = "YOUR_32_CHARACTER_DATABASE_ID"
TAVILY_API_KEY = "tvly-YOUR_TAVILY_KEY"
OPENROUTER_API_KEY = "sk-or-v1-YOUR_OPENROUTER_KEY"
SLACK_WEBHOOK_URL = "https://hooks.slack.com/services/YOUR/WEBHOOK/URL"
